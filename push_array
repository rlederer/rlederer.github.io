    <button id="submit-element" type="button" name="button">Hey I'm a button!</button>
    <input id="input-element" type="text" name="">

    <script type="text/javascript">
      let input = document.getElementById('input-element');
      let button = document.getElementById('submit-element');

      let arr = [];

      button.addEventListener('click', () => {

          arr.push(input.value);
          console.log(arr);
      });
    </script>

